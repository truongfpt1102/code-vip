extends ../layouts/staffMenu
block content
  
        .card-body
            h5.card-title Manage account
            button.btn(type='button' data-toggle='modal' data-target='#exampleModal') Go
            // Modal
            #exampleModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
                .modal-dialog(role='document')
                    .modal-content
                        .modal-header
                            h5#exampleModalLabel.modal-title Select the object to create an account
                            button.close(type='button' data-dismiss='modal' aria-label='Close')
                            span(aria-hidden='true') Ã—
                        .modal-footer
                            a.btn(href = '/staff/createAccountTrainee') Trainee


